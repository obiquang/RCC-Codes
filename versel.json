{
    "version":2,
    "builds":[ { "src":"./server.js", "use"  :"@versel/node" } ],
    "routes":[ 
        {
            "src":"/(.*)",
            "dest" :"/"
        },
        {
            "src" : "/",
            "dest": "/codes"
        },
        {
            "src" : "/codes",
            "dest": "/codes"
        }
    ]
}